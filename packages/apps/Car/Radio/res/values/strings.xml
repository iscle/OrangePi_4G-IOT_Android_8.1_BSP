<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The main application name for the radio. -->
    <string name="app_name">Radio</string>

    <!-- The name for the service that is responsible for connecting to the radio API. -->
    <string name="service_name">RadioService</string>

    <!-- Text to denote the AM radio band. -->
    <string name="radio_am_text">AM</string>

    <!-- Text to denote the FM radio band. -->
    <string name="radio_fm_text">FM</string>

    <!-- Transition name for the radio control background to current radio card in the presets
         list . -->
    <string name="radio_controls_transition_name" translatable="false">
        radio_controls_transition</string>

    <!-- Transition name for the play button that will be shared between the main radio
         display and presets list. -->
    <string name="radio_play_button_transition_name" translatable="false">
        radio_play_button_transition</string>

    <!-- Text indicating to the user that there was an error loading the error. -->
    <string name="radio_problem_text">Radio not available</string>

    <!-- Default metadata text if a preset has no metadata. This text should indicate to the user
         that the station is favorited followed by the number of the favorite. -->
    <string name="radio_default_preset_metadata_text">Favorite <xliff:g>%1$s</xliff:g></string>

    <!-- The text in the drawer that will take the user to the radio's manual tuner. -->
    <string name="manual_tuner_drawer_entry">Manual Tuner</string>

    <!-- The strings for the dialpad in the manual tuner. -->
    <string name="manual_tuner_0">0</string>
    <string name="manual_tuner_1">1</string>
    <string name="manual_tuner_2">2</string>
    <string name="manual_tuner_3">3</string>
    <string name="manual_tuner_4">4</string>
    <string name="manual_tuner_5">5</string>
    <string name="manual_tuner_6">6</string>
    <string name="manual_tuner_7">7</string>
    <string name="manual_tuner_8">8</string>
    <string name="manual_tuner_9">9</string>
    <string name="manual_tuner_period">.</string>
</resources>

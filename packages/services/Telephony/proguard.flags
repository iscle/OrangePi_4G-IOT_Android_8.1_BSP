# Keep classes and methods that have the guava @VisibleForTesting annotation
-keep @**.VisibleForTesting class *
-keepclassmembers class * {
@**.VisibleForTesting *;
}
-keep @**.NeededForTesting class *
-keepclassmembers class * {
@**.NeededForTesting *;
}
-verbose
-keep class com.android.services.telephony.* {
 *;
}
-keep class com.android.phone.settings.SettingsConstants {
 *;
}
-keep class com.android.phone.* {
 *;
}
-keep class com.android.phone.common.* {
 *;
}

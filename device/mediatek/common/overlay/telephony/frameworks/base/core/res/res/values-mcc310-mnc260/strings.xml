        <item>1301</item>    <!-- CODE_WFC_INCORRECT_SIM_CARD_ERROR -->
        <item>1401</item>    <!-- CODE_WFC_LOCAL_OR_NULL_PTR_ERROR -->
        <item>1081</item>    <!-- CODE_WFC_EPDG_CON_OR_LOCAL_OR_NULL_PTR_ERROR -->
